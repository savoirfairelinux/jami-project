[access]
	inheritFrom = ring
[project]
	description = Ring - http://www.ring.cx/\n\
An all-in-one repository to obtain all Ring sources.
[submit]
	action = rebase if necessary
[access "refs/heads/integration"]
	push = +force group Release
	pushMerge = group Release
	forgeAuthor = group Release
	forgeCommitter = group Release
[access "refs/for/refs/heads/integration"]
	push = group Release
	pushMerge = group Release
[access "refs/heads/*"]
	label-Verified = -1..+1 group project-verifiers
